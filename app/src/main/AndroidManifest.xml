<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.hostelproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.ImageActivity" />
        <activity android:name=".ui.ListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.EnterDataActivity" />
        <activity android:name=".ui.DetailActivity" />
        <activity android:name=".FireBaseAccountsUtils.LogIn" />
        <activity android:name=".FireBaseAccountsUtils.SignUp" />

    </application>

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>